{
  "mongodb_prod": {
    "host": "mongo",
    "port": 27017,
    "database": "mongo-db1",
    "username": "admin",
    "password": "",
    "name": "mongodb_prod",
    "connector": "mongodb"
  },
  "userService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "GET",
        "url": "http://projetm2gla.istic.univ-rennes1.fr/api/SITUsers/{userId}/accessTokens/{token}",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json",
          "Host": "user.docker.localhost"
        }
      },
      "functions": {
        "checkAuth": ["userId", "token"]
      }
    }]
  },
  "simulatorService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "POST",
        "url": "{url}",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json"
        },
        "body": {
          "instance": "{instance}",
          "location": "{location}"
        }
      },
      "functions": {
        "createDrone": ["url","instance","location"]
      }
    }]
  },
  "simulatorService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "POST",
        "url": "{url}",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json"
        },
        "body": {
          "instance": "{instance}",
          "mission": "{mission}",
          "intervention": "{intervention}"
        }
      },
      "functions": {
        "startMission": ["url","instance","mission","intervention"]
      }
    }]
  }
}
