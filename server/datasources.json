{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "pushService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "POST",
        "url": "http://localhost:3009/api/pushMessages",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json",
          "Host": "push.docker.localhost"
        },
        "body": "{data}"
      },
      "functions": {
        "create": ["data"]
      }
    }]
  },
  "simulatorService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "POST",
        "url": "{url}/create",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json"
        },
        "body": {
          "instance": "{instance}",
          "home": "{location}"
        }
      },
      "functions": {
        "createDrone": ["url","instance","location"]
      }
    },
    {
      "template": {
        "method": "POST",
        "url": "{url}/mission",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json"
        },
        "body": {
          "id": "{id}",
          "instance": "{instance}",
          "mission": "{mission}",
          "intervention": "{intervention}"
        }
      },
      "functions": {
        "startMission": ["url","id", "instance","mission","intervention"]
      }
    }]
  }
}
