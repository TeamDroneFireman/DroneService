{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "userService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "GET",
        "url": "http://localhost:3000/api/SITUsers/{userId}/accessTokens/{token}",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json",
          "Host": "user.docker.localhost"
        }
      },
      "functions": {
        "checkAuth": ["userId", "token"]
      }
    }]
  },
  "interventionService": {
    "connector": "rest",
    "debug": "false",
    "operations": [{
      "template": {
        "method": "POST",
        "url": "http://projetm2gla.istic.univ-rennes1.fr/api/Interventions/{intervId}/push/",
        "headers": {
          "accepts": "application/json",
          "content-type": "application/json"
        },
        "body": {
          "id": "{id}",
          "topic": "{topic}"
        }
      },
      "functions": {
        "push": ["intervId","id","topic"]
      }
    }]
  }
}
